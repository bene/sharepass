{
    "manifest_version": 3,
    "name": "SharePass",
    "version": "NPM_VERSION",
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "#options_page": "options.html",
    "content_scripts": [
        {
            "js": ["main.js"],
            "css": ["main.css"],
            "matches": ["https://*/*", "http://*/*"]
        }
    ]
}
